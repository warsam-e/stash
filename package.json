{
	"name": "@warsam-e/stash",
	"description": "A simple and flexible caching library for TypeScript",
	"version": "1.2.0",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"type": "module",
	"license": "MIT",
	"files": [
		"dist"
	],
	"scripts": {
		"build:lib": "tsdown",
		"build:docs": "typedoc --options typedoc.json",
		"build": "bun build:lib && bun build:docs",
		"dev": "bun test --watch",
		"check": "bunx @biomejs/biome check"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"@types/bun": "1.3.1",
		"tsdown": "0.15.12",
		"typedoc-github-theme": "0.3.1",
		"typescript": "5.9.3"
	},
	"dependencies": {
		"bun.db": "1.1.4",
		"chrono-node": "2.9.0",
		"redis": "5.9.0"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/warsam-e/stash.git"
	},
	"bugs": {
		"url": "https://github.com/warsam-e/stash/issues"
	},
	"homepage": "https://warsame.me/stash"
}
